doctype 5
html
  head
    title= title
    link(rel='stylesheet', href='/stylesheets/style.css')
    link(rel='stylesheet', href='/stylesheets/bootstrap/bootstrap.min.css')    
    link(rel='stylesheet', href='/stylesheets/bootstrap/customBootstrap.css')
  body
    div(class="container")
      div(class="navbar")
        div(class="navbar-inner")
            ul(class="nav")
              li
                a(href="/") Home
              li
                a(href="/record") Track Records                
              li
                a(href="/about") About Us
            a(class="brandRight",id="userName") #{patientName}

      div(class="container-fluid")    
  
   
    block content
    
    
    <!--JS-->
    script(src="/js/lib/jquery-min.js")
        
    <!--Socket.io-->
    script(src="/socket.io/socket.io.js")
    
    <!--bootstrap.js is being run when requireJS is first loaded-->
    script(type="text/javascript", data-main="/js/bootstrap", src="/js/lib/requirejs/require-min.js").
